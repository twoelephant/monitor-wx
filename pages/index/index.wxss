/* 登录块样式 */
.falogin{
	position: fixed;
	top: 0;
	min-height: 100%;
	width: 100%;
	background-color: black;
	opacity: 0.7;
	z-index: 100;
}
.login{
	position: fixed;
	top: 30%;
	left: 10%;
	background-color: #ffffff;
	width: 80%;
	/* height: 60%; */
	opacity: 1;
	color: #969696;
	border-radius: 20rpx;
	z-index: 200;

}
.login .faimage1{
	width: 100%;
	height: 60rpx;

}
.login .faimage1 .image1{
	display: block;
	width: 60rpx;
	height: 60rpx;
	
	float: right;
	margin: 5rpx 5rpx;
}
.login  .image2{
	display: block;
	width: 400rpx;
	height: 400rpx ;
	background-color: red;
	margin: 10rpx auto ;
}
.login .lr1{
	width: 40%;
	margin: 0 auto;
	padding: 10rpx 0;
	text-align: center;
}
.login button{
	background-color: #ec5d63;
	color: #ffffff;
	margin: 30rpx auto;
	border-radius: 40rpx;
}

